<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.iushu.hibernate.mysql.entity.Department" table="department">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="name" column="name" type="java.lang.String"/>
        <property name="manager" column="manager" type="java.lang.Integer"/>
        <property name="update_time" column="update_time" type="java.util.Date"/>
        <property name="create_time" column="create_time" type="java.util.Date"/>
    </class>

</hibernate-mapping>